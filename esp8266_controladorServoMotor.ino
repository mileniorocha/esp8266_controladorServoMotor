//INCLUIR BIBLIOTECAS DO ESP-8266 E DO SERVO MOTOR
#include <ESP8266WiFi.h>
#include <Servo.h>

//DECLARAR CREDENCIAIS DE REDE
const char* ssid = "nomedowifi";
const char* password = "senhadowifi";

//INICIALIZA SERVIDOR WIFI E O SERVO MOTOR
WiFiServer server(80);
Servo myServo;
int pinoServo = 2;
int anguloServo = 90;

void setup() {
  //INICIALIZA SERIAL (ONDE IRÁ MOSTAR O IP DO ESP-8266)
  Serial.begin(115200);
  WiFi.begin(ssid, password);
  //FAZ A CONEXÃO NO WIFI
  while (WiFi.status() != WL_CONNECTED) {
    delay(1000);
    Serial.println("Conectando ao Wi-Fi...");
  }
  server.begin();
  Serial.println("Servidor iniciado!");
  myServo.attach(pinoServo);
  myServo.write(anguloServo);
}

void loop() {
  WiFiClient client = server.available();
  if (client) {
    Serial.println("Nova conexão!");
    while (client.connected()) {
      if (client.available()) {
        String request = client.readStringUntil('\r');
        Serial.println(request);
        //VERIFICA QUAL O ANGULO DESEJADO E MOVE O SERVO NO ANGULO QUE FOI CLICADO
        if (request.indexOf("/angulo/0") != -1) {
          anguloServo = 0;
          myServo.write(anguloServo);
        }
        if (request.indexOf("/angulo/90") != -1) {
          anguloServo = 90;
          myServo.write(anguloServo);
        }
        if (request.indexOf("/angulo/180") != -1) {
          anguloServo = 180;
          myServo.write(anguloServo);
        }

        //PÁGINA HTML
        client.println("HTTP/1.1 200 OK");
        client.println("Content-Type: text/html");
        client.println("");
        client.println("<!DOCTYPE HTML>");
        client.println("<html>");
        client.println("<head><title>Controlando Servo Motor</title>");
        client.println("<style> ");
        client.println("a:link { color: blue; background-color: transparent; text-decoration: none; }");
        client.println("a:hover { color: black; background-color: transparent; text-decoration: underline; }");
        client.println("</style>");
        client.println("</head>");
        client.println("<body>");
        client.println("<h1>Controle do Servo Motor - ESP8266</h1>");
        client.println("<h3>Angulo atual: " + String(anguloServo) + "</h3>");
        client.println("<p><a href=\"/anguloServo/0\">Mover para 0 graus</a></p>");
        client.println("<p><a href=\"/anguloServo/90\">Mover para 90 graus</a></p>");
        client.println("<p><a href=\"/anguloServo/180\">Mover para 180 graus</a></p>");
        //O CÓDIGO ABAIXO É UMA IMAGEM EM BASE64, CASO DESEJA INCLUIR OUTRA IMAGEM EM BASE64 BASTA TROCAR O CÓDIGO PELA IMAGEM DESEJADA (NÃO PODE SER UMA IMAGEM MUITO GRANDE)
        client.println("<img src='data:image/png;base64,'>");
        client.println("</body>");
        client.println("</html>");
        break;
      }
    }
    delay(1);
    client.stop();
    Serial.println("Conexão fechada!");
  }
}
